<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="ScrollRefreshLayout">
        <attr name="layout_scroll_empty" format="reference"/>
        <attr name="text_error_tip" format="string"/>
    </declare-styleable>

    <declare-styleable name="RefreshLayout">
        <attr name="layout_refresh_empty" format="reference"/>
        <attr name="layout_refresh_error" format="reference"/>
        <attr name="layout_refresh_loading" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WebViewLayout">
        <attr name="isUpdateTitle" format="boolean" />
        <attr name="isShowIconBack" format="boolean" />
        <attr name="isJavaScriptEnabled" format="boolean" />
    </declare-styleable>
    <!--自动换行Linearlayout-->
    <declare-styleable name="WarpLinearLayout">
        <attr name="grivate" format="enum"><!--对齐方式 !-->
            <enum name="right" value="0"></enum>
            <enum name="left" value="1"></enum>
            <enum name="center" value="2"></enum>
        </attr>
        <attr name="horizontal_Space" format="dimension"></attr>
        <attr name="vertical_Space" format="dimension"></attr>
        <attr name="isFull" format="boolean"></attr>
        <attr name="maxLines" format="integer"></attr><!--  0表示无限制-->
        <attr name="expandviewId" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="CustomScrollView">
        <attr name="maxY" format="integer" />
        <attr name="maxX" format="integer" />
        <attr name="validDistance" format="integer" />
    </declare-styleable>

    <declare-styleable name="YDXSeekBar">
        <attr name="seekbarNightmode" format="boolean" />
        <attr name="android:thumb"/>
        <attr name="android:progressDrawable"/>
        <attr name="android:layout_width"/>
        <attr name="progressHight" format="dimension"/>
        <attr name="thumbSize" format="dimension"/>
        <attr name="showProgresstext" format="boolean"/>
        <attr name="showAnchor" format="boolean"/>
        <attr name="anchorSize" format="dimension"/>
        <attr name="anchorDrawable" format="reference"/>
        <attr name="anchorNightDrawable" format="reference"/>
        <attr name="progressDrawableLeftSize" format="dimension"/>
        <attr name="progressDrawableRightSize" format="dimension"/>
        <attr name="progressDrawableLeft" format="reference"/>
        <attr name="progressDrawableRight" format="reference"/>
        <attr name="progressDrawableLeftNight" format="reference"/>
        <attr name="progressDrawableRightNight" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DayNightView">
        <attr name="cutLineNightmode" format="boolean"/>
        <attr name="cutlineColor" format="reference" />
        <attr name="cutlineNightColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="DayNightTextView">
        <attr name="textNightmode" format="boolean"/>
        <attr name="nightTextColor" format="color"/>
        <attr name="nightText" format="string"/>
        <attr name="dayText" format="string"/>
        <attr name="nightBackground" format="reference"/>
        <attr name="nightDrawableLeft" format="reference"/>
        <attr name="nightDrawableRight" format="reference"/>
        <attr name="nightDrawableTop" format="reference"/>
        <attr name="nightDrawableBottom" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DayNightRadioButton">
        <attr name="radioNightmode" format="boolean"/>
        <attr name="nightRadioColor" format="color"/>
        <attr name="nightRadioText" format="string"/>
        <attr name="nightRadioBackground" format="reference"/>
        <attr name="nightRadioDrawableLeft" format="reference"/>
        <attr name="nightRadioDrawableRight" format="reference"/>
        <attr name="nightRadioDrawableTop" format="reference"/>
        <attr name="nightRadioDrawableBottom" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DayNightImageView">
        <attr name="ImageNightmode" format="boolean"/>
        <attr name="ImageNightSrc" format="reference"/>
        <attr name="ImageNightBackground" format="reference"/>
    </declare-styleable>
    <declare-styleable name="DayNightSeekBar">
        <attr name="seekBarNightmode" format="boolean"/>
        <attr name="nightProgressDrawable" format="reference"/>
        <attr name="nightThumb" format="reference"/>
    </declare-styleable>



    <declare-styleable name="ydxReadStyle">
        <attr name="pageTextColor" format="color" />
        <attr name="pageBackgroundColor" format="color" />
        <attr name="dialogTextColor" format="color" />
        <attr name="dialogBackgroundColor" format="color" />
        <attr name="progressProgressColor" format="color" />
        <attr name="progressBackgroundColor" format="color" />
        <attr name="progressThumbColor" format="color" />
        <attr name="backicon" format="reference" />
    </declare-styleable>

    <declare-styleable name="StyleTextView">
        <attr name="readStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandIndicator" format="reference"/>
        <attr name="collapseIndicator" format="reference"/>
        <attr name="expandToggleType" format="enum">
            <enum name="ImageButton" value="0"/>
            <enum name="TextView" value="1"/>
            <enum name="Container" value="2"/>
        </attr>
        <attr name="expandableTextId" format="reference"/>
        <attr name="expandCollapseToggleId" format="reference"/>
        <attr name="expandToggleOnTextClick" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="FastScrollRecyclerView">
        <attr name="fastScrollThumbColor" format="reference|color" />
        <attr name="fastScrollTrackColor" format="reference|color" />
        <attr name="fastScrollPopupBgColor" format="reference|color" />
        <attr name="fastScrollPopupTextColor" format="reference|color" />
        <attr name="fastScrollPopupTextSize" format="reference|dimension" />
        <attr name="fastScrollPopupBackgroundSize" format="reference|dimension" />
        <attr name="fastScrollPopupPosition" format="enum">
            <enum name="adjacent" value="0"/>
            <enum name="center" value="1"/>
        </attr>
        <attr name="fastScrollAutoHide" format="reference|boolean" />
        <attr name="fastScrollAutoHideDelay" format="reference|integer" />
        <attr name="fastScrollThumbInactiveColor" format="reference|boolean" />
        <attr name="fastScrollThumbDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="YDXRatingBar" >
        <!--默认图片-->
        <attr name="statrNormal" format="reference"/>
        <!--选中图片-->
        <attr name="statrFocus" format="reference"/>
        <!--星星总数量-->
        <attr name="totalNumner" format="integer"/>
        <!--星间隔-->
        <attr name="intervalNumner" format="dimension"/>
        <!--是否可以选中-->
        <attr name="isSelect" format="boolean"/>
        <!--选中星星数量-->
        <attr name="selectNumber" format="integer"/>
        <!--图片尺寸-->
        <attr name="starWidth" format="dimension"/>
        <attr name="starHeight" format="dimension"/>
    </declare-styleable>
</resources>